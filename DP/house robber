class Solution {
    int[] dp ;
    public int rob(int[] nums) {
        dp = new int[nums.length+1];   
        Arrays.fill(dp, -1);
        return rob(nums.length-1, nums);
}
public int rob(int n, int[] nums){

    if(n<0) return 0;
    if(dp[n]!=-1) return dp[n];
  
        int res = Math.max(nums[n] + rob(n-2, nums), rob(n-1, nums));
        dp[n] = res;

    return dp[n];
}

}
